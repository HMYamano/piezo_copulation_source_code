            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="rx:Zip" />
            </Expression>
            <Expression xsi:type="scr:PythonTransform">
              <scr:Script>@returns(float)
def process(value):
  a = value.Item1
  b = value.Item2
  position = (a + b) / 2
  return position</scr:Script>
            </Expression>
            <Expression xsi:type="MemberSelector">
