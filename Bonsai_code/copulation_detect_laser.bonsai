            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="rx:CombineLatest" />
            </Expression>
            <Expression xsi:type="scr:PythonTransform">
              <scr:Script>@returns (int)
def process(value):
  distance = value.Item1
  threshold = value.Item2
  global inside  

  if (distance &lt;= threshold):
    inside = True
    judge = 1
    return judge
  else:
    inside = False
    judge = 0
    return judge
</scr:Script>
