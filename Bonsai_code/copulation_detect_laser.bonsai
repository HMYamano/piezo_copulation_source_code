    judge = 0
    return judge
</scr:Script>
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="rx:Zip" />
            </Expression>
            <Expression xsi:type="scr:PythonTransform">
              <scr:Script>@returns (bool)
def process(value):
  jud1 = value.Item1
  jud2 = value.Item2
  
  jud = jud1 + jud2
  if (jud &gt;= 2):
    kint = True
    return kint
  else:
    kint = False
    return kint
</scr:Script>
